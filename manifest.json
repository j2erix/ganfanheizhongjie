{
    "manifest_version": 2,
    "name": "干翻黑中介",
    "version": "1.0",
    "description": "黑中介滚出互联网",
    "icons": {
        "16": "images/hei-logo.png",
        "48": "images/hei-logo.png",
        "128": "images/hei-logo.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/hei-logo.png",
            "38": "images/hei-logo.png"
        },
        "default_title": "干翻黑中介"
    },
    "content_scripts": [
        {
            "matches": ["*://*.ganji.com/*", "*://*.58.com/*"],
            "css": ["css/main.css"],
            "js": ["js/jquery-1.11.3.min.js", "js/main.js"]
        }
    ],
    "permissions": [
    ]
}